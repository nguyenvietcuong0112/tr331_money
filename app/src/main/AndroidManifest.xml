<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.finance.trackmoney.dailyspend.moneymanager.moneytracker" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        android:enableOnBackInvokedCallback="true"
        tools:targetApi="31">
        <activity
            android:name=".activity.SplashActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".activity.InterestActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />

        <activity
            android:name=".utils.ActivityLoadNativeFullLanguage"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TransactionDetailActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.TrackAllYourSpendingActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />


        <activity
            android:name=".activity.CurrencyUnitActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />


        <activity
            android:name=".activity.AddTransactionActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />

        <activity
            android:name=".ads.ActivityLoadNativeFullV2"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.BudgetDetailActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.IntroActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragment.HomeActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.LanguageActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.StatisticDetailActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragment.StatisticsActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragment.BudgetActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".fragment.SettingsActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SeeAllActivity"
            android:theme="@style/Theme.Design.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".utils.LoadNativeFullNew"
            android:screenOrientation="portrait" />

        <activity
            android:name=".utils.LoadNativeFull"
            android:theme="@style/Theme.Design.NoActionBar"
            android:exported="true"
            android:screenOrientation="portrait" />

        <activity
            android:name=".utils.ActivityLoadNativeFull"
            android:theme="@style/Theme.Design.NoActionBar"
            android:exported="true"
            android:screenOrientation="portrait" />


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />
        <meta-data
            android:name="com.facebook.sdk.AutoInitEnabled"
            android:value="true" />
        <meta-data
            android:name="com.facebook.sdk.AutoLogAppEventsEnabled"
            android:value="true" />
        <meta-data
            android:name="com.facebook.sdk.AdvertiserIDCollectionEnabled"
            android:value="true" />
        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />
    </application>

</manifest>